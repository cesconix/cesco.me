{
  "name": "cesco.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' dotenv -- next dev",
    "build": "dotenv -- next build",
    "start": "dotenv -- next start",
    "lint": "dotenv -- next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "codegen": "graphql-codegen --require dotenv/config --config graphql.config.js",
    "codegen:watch": "pnpm run codegen --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@tanstack/react-query": "^4.29.19",
    "@types/node": "20.4.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "clsx": "^2.0.0",
    "datocms-structured-text-utils": "^2.0.4",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "graphql-request": "^6.1.0",
    "next": "13.4.9",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-datocms": "^4.1.3",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@next/bundle-analyzer": "^13.4.10",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0"
  },
  "packageManager": "pnpm",
  "engine": {
    "node": ">18"
  }
}
